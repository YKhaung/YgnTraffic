<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item>
    <shape
      android:dither="true"
      android:shape="oval">

      <solid android:color="#11333333"/>
      <size
        android:width="55dp"
        android:height="55dp"/>
    </shape>
  </item>

  <item
    android:bottom="2dp"
    android:left="2dp"
    android:right="2dp"
    android:top="2dp">
    <shape
      android:dither="true"
      android:shape="oval">

      <corners android:radius="20dp"/>

      <solid android:color="#fff"/>

    </shape>
  </item>
</layer-list>